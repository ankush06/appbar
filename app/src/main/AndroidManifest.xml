<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.desibazar.appbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_search_white_24dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.desibazar.appbar.SettingsActivity"
            android:label="@string/settings_lebel"
            android:parentActivityName="com.example.desibazar.appbar.MainActivity" >

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.desibazar.appbar.MainActivity" />
        </activity>

        <activity
            android:name="com.example.desibazar.appbar.BaseToolbarActivity"
            android:label="settings">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.desibazar.appbar.BaseToolbarActivity" />
        </activity>
    </application>

</manifest>